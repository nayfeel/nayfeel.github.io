                  <animateTransform attributeName="transform" type="translate" keyTimes="0;1" values="-100 426.2948690121617;1536 426.2948690121617" dur="16.666666666666668s" repeatCount="indefinite" begin="-58.398104984615856s"></animateTransform>
                  <path d="M84.717,33.597c0.791-2.503,1.186-5.138,1.186-7.773C85.903,11.594,74.308,0,60.079,0 c-9.881,0-18.445,5.534-22.793,13.702c-1.581-0.527-3.426-0.791-5.138-0.791c-9.486,0-17.128,7.642-17.128,17.128 c0,1.186,0.132,2.372,0.395,3.426C6.719,34.783,0,42.424,0,51.515C0,61.66,8.169,69.829,18.314,69.829h63.373 C91.831,69.829,100,61.66,100,51.515C99.868,42.556,93.281,35.046,84.717,33.597z" fill="#bddeff" transform="scale(1)"></path>
                </g><g transform="translate(-100 576.332167665955) rotate(0)">
                  <animateTransform attributeName="transform" type="translate" keyTimes="0;1" values="-100 576.332167665955;1536 576.332167665955" dur="16.666666666666668s" repeatCount="indefinite" begin="-65.9688371733193s"></animateTransform>
                  <path d="M84.717,33.597c0.791-2.503,1.186-5.138,1.186-7.773C85.903,11.594,74.308,0,60.079,0 c-9.881,0-18.445,5.534-22.793,13.702c-1.581-0.527-3.426-0.791-5.138-0.791c-9.486,0-17.128,7.642-17.128,17.128 c0,1.186,0.132,2.372,0.395,3.426C6.719,34.783,0,42.424,0,51.515C0,61.66,8.169,69.829,18.314,69.829h63.373 C91.831,69.829,100,61.66,100,51.515C99.868,42.556,93.281,35.046,84.717,33.597z" fill="#bddeff" transform="scale(1)"></path>
                </g></g>
                </svg>
            </div>
            <div class="layer_3" id="layer_3">
                <div class="game__preloader" id="preloader"></div>
                <div class="game__modal" id="modal" style="display: none">
                    <div class="game__title" id="text"></div>
                    <div class="game__progressbar" id="progressbar">
                        <div class="game__progressbar-fill" id="progressbar-fill"></div>
                    </div>
                </div>
                <div class="game__kef" id="kef" style="display: block;"></div>
                <div class="game__message-wrap" id="message-wrap"></div>
                <div class="game__person game__person--plane" id="plane" style="display: none"></div>
                <div class="game__person game__person--rocket" id="rocket" style="display: none"></div>
            </div>
        </div>
    `);
}

setHtml();

function Aviator() {
    $('#graph').hide();

    setHtml();
    return {
        _animPlane: null,
        _animPreloader: null,
        _animRocket: null,

        _$layer_3: $('#layer_3'),
        _$layer_2: $('#layer_2'),
        _$layer_1: $('#layer_1'),

        $kef: $('#kef'),

        _$rocket: $('#rocket'),
        _$person: $('.game__person'),
        _$plane: $('#plane'),
        _$preloader: $('#preloader'),
        _$progressbar: $('#progressbar'),
        _$progressbarFill: $('#progressbar-fill'),
        _$text: $('#text'),
        _$modal: $('#modal'),

        MARGIN_LEFT_SMALL: 160,

        screen_game_init(time_wait = 5) {
            if(this._animPreloader) {
                this._animPreloader.stop();
                this._animPreloader.destroy();
            }
            this._animPreloader = lottie.loadAnimation({
                container: this._$preloader[0],
                renderer: 'svg',
                loop: true,
                autoplay: true,
                animationData: preloaderData
            });

            this._$preloader.show();


            PAUSE_NOW_X = true;

            this._$text.html("Ожидание запуска");
            this._$modal.show();
            this._$layer_2.find('svg').css({'top': '-43%', 'transition': 'top 0.5s'});


            this.disconnect(false);
            this._$layer_2.css('opacity', 1);

            this._$rocket.hide();
            this._$plane.hide();

            if(this._animRocket) {
                console.log('rocket destroy')
                this._animRocket.stop();
                this._animRocket.destroy();
            }

            this._$person.css({
                left: '5px',
                top: '100%',
                transform: 'translate(0, 0)'
            });

            // this._$bg.css('filter', 'blur(0.25rem)');
            this.$kef.hide();
            this._$progressbarFill.stop().css("width", 0);
            this._$progressbarFill.animate({
                width: '100%'
            }, time_wait * 1000, function () {
                $(this).removeAttr('style');
            });
        },

        screen_game_start() {
            PAUSE_NOW_X = false;
            this._$progressbarFill.stop().css("width", 0);


            if(this._animPlane) {
                this._animPlane.stop();
                this._animPlane.destroy();
            }

            this._animPlane = lottie.loadAnimation({
                container: this._$plane[0],
                renderer: 'svg',
                loop: true,
                autoplay: true,
                animationData: planeData
            });

            this._$layer_2.find('svg').css({'top': 0, 'transition': 'top 5s'});
            this._$layer_2.css('opacity', 1);

            this._$preloader.hide();
            this._$modal.hide();

            this.$kef.show();
            this._$plane.show();

            this._$person.css({
                left: '50%',
                top: '50%',
                transform: 'translate(-50%, -50%)'
            });

            //TODO Delete
            // this._animRocket = lottie.loadAnimation({
            //     container: this._$rocket[0],
            //     renderer: 'svg',
            //     loop: true,
            //     autoplay: true,
            //     animationData: rocketData
            // });
            // this._$rocket.show();
        },

        screen_game_restart(time_wait) {
            this.screen_game_init(time_wait);
        },

        screen_game_failed(curr_kef) {
            PAUSE_NOW_X = true;

            this._$layer_2.find('svg').css({'top': '-43%', 'transition': 'top 5s'});
            this._$layer_2.css('opacity', 1);

            if(typeof curr_kef !== 'undefined') {
                curr_kef = parseFloat(curr_kef);
                this.$kef.text(Math.round(NOW_X*100) /100 + ' X');
            }

            this._$person.css({
                'top': '-20%',
                'left': '140%'
            });
        },

        screen_game_view(str) {
            if(str === "small" ) {
                this._$layer_3.css({
                    'width': `calc(100% - ${this.MARGIN_LEFT_SMALL}px)`,
                    'right' : '0'
                });
            }
            else if(str === "full") {
                this._$layer_3.css({'width': '100%'});
            }
        },

        screen_game_supergame() {
            this._$layer_2.css('opacity', 0);
            this._$plane.hide();
            this._$rocket.show();

            if(this._animPlane) {
                this._animPlane.stop();
                this._animPlane.destroy();
            }

            if(this._animRocket) {
                this._animRocket.stop();
                this._animRocket.destroy();
            }

            this._animRocket = lottie.loadAnimation({
                container: this._$rocket[0],
                renderer: 'svg',
                loop: true,
                autoplay: true,
                animationData: rocketData
            });

            this._$layer_2.find('svg').css({'top': '100%', 'transition': 'top 2s ease-in'});
        },

        disconnect(is_modal = true) {
            const message_wrap = $('#message-wrap');
            if(is_modal) {
                message_wrap.show();
                message_wrap.empty().append(`
                <div class="game__message" id="message">
                    <div class="game__text" id="text">Connection is lost</div>
                    <div class="outside-light">
                        <button onclick="location.reload()" class="game__button" id="button">REFRESH PAGE</button>
                    </div>
                </div>`);

                $('#game').css({
                    'background-color': 'rgba(0,0,0,.9)',
                    'filter': 'blur(0.5rem)'
                });
                $('#kef').css({
                    'filter': 'blur(0.5rem)'
                });
            }
            else {
                message_wrap.hide();
            }
        }
    }
}
